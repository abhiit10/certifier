<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="exampleHibernateJPA" transaction-type="RESOURCE_LOCAL">
		<class>com.certifier.rest.entity.CAnswer</class>
		<class>com.certifier.rest.entity.COption</class>
		<class>com.certifier.rest.entity.CQuestion</class>
		<class>com.certifier.rest.entity.CTest</class>
		<class>com.certifier.rest.entity.CUser</class>
		<class>com.certifier.rest.entity.CUserHistory</class>
		<class>com.certifier.rest.entity.CUserHistoryDetail</class>
		<class>com.certifier.rest.entity.CUserProfile</class>
		<properties>
<!-- 			<property name="hibernate.dialect" value="hibernate.dialect.MySQLDialect"/> -->
			<property name="hibernate.ejb.cfgfile" value="/hibernate.cfg.xml" />
		</properties>
	</persistence-unit>
</persistence>
